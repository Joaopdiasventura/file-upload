generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  email    String @id @map("_id")
  name     String
  password String
  File     File[]
}

model File {
  id            String @id @default(auto()) @map("_id") @db.ObjectId
  user          String @map("fk_user_email")
  metadata      Json
  fk_user_email User   @relation(references: [email], fields: [user])
}